{
    "name": "svn-scm-vscode",
    "displayName": "svn-scm-vscode",
    "description": "SVN SCM extension for vscode",
    "version": "0.0.1",
    "publisher": "bvandre",
    "engines": {
        "vscode": "^1.11.0"
    },
    "categories": [
        "SCM Providers"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "SVN",
            "properties": {
                "svn.enabled": {
                    "type": "boolean",
                    "description": "Whether svn is enabled",
                    "default": true
                },
                "svn.path": {
                    "type": "string",
                    "description": "Path to the svn executable",
                    "default": null
                }
            }
        }
    },
    "dependencies": {
        "vscode": "^1.0.0",
        "vscode-nls": "^2.0.1"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "svn-spawn": "^0.1.5",
        "mocha": "^2.3.3",
        "iconv-lite": "0.4.15",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}